<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="main">
<item name="android:background">@drawable/background</item>
       <item name="android:layout_width" >match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="main_circle">
        <item name="android:layout_width">73dp</item>
       <item name="android:layout_height">62dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:background">@drawable/toolbar</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/circle</item>

    </style>

    <style name="white_button">

        <item name="android:layout_width">161dp</item>
        <item name="android:layout_height">205dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:background">@drawable/toolbar</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/white_border</item>

    </style>

    <style name="white_button_small">

        <item name="android:layout_width">160dp</item>
        <item name="android:layout_height">119dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:background">@drawable/toolbar</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:src">@drawable/white_border</item>

    </style>
    <style name="toolbar">
        <item name="android:layout_width">42dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:background">@drawable/toolbar</item>
        <item name="android:scaleType">fitXY</item>

    </style>
    <style name="category_icons">
        <item name="android:layout_width">123dp</item>
        <item name="android:layout_height">121dp</item>
        <item name="android:background">@drawable/toolbar</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="toolbar_background">
       <item name="android:layout_width">418dp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:src">@drawable/graybox</item>

    </style>
    <style name="images_in_button">
        <item name="android:layout_width">140dp</item>
        <item name="android:layout_height">81dp</item>
        <item name="android:layout_marginTop">8dp</item>

    </style>



</resources>